  "scripts": {
    "download": "node bin/download_dax_dukascopy_5min.js",
    "download:5min": "node bin/download_dax_dukascopy_5min.js",
    "download:1min": "node bin/download_dax_dukascopy_1min.js",
    "download:year": "node bin/download_dax_dukascopy_5min.js --year 2024",
    "download:daily": "node bin/download_dax_dukascopy_5min.js --daily",
    "analyze-gaps": "node bin/analyze_data_gaps.js",
